    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\" --names server,client"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "express-validator": "^6.13.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.12",
    "morgan": "~1.9.1",
    "multer": "^1.4.3",
    "pug": "2.0.0-beta11",
    "react-router-dom": "^6.0.2",
    "vcards-js": "^2.10.0"
  },
  "devDependencies": {
    "concurrently": "^6.3.0",
    "nodemon": "^2.0.14"
  }
}
